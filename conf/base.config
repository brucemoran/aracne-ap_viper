/*
 * -------------------------------------------
 *  Nextflow base config file for exome_n-of-1
 * -------------------------------------------
 * Imported under the default 'standard' Nextflow
 */

params {

  //system-specific
  max_memory = 128.GB
  max_cpus = 16
  max_time = 240.h

  binDir = "$baseDir/bin"

}

process {

  container = params.container

}
